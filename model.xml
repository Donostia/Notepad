<?xml version="1.0" encoding="UTF-8" standalone="no"?><feature-model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.tdg-seville.info/benavides/featuremodelling/feature-model.xsd">
	<feature name="Notepad">
	<binaryRelation name="BR-1">
		<cardinality max="1" min="0"/>
		<solitaryFeature name="WordCount">
			<setRelation name="SR-1">
				<cardinality max="3" min="1"/>
				<groupedFeature name="CharacterCount"/>
				<groupedFeature name="NonBlancCharCount"/>
				<groupedFeature name="WordCounts"/>
			</setRelation>
		</solitaryFeature>
	</binaryRelation>
	<binaryRelation name="R-Notepad"><cardinality max="1" min="0"/><solitaryFeature name="Cut"><binaryRelation name="R-Cut"><cardinality max="1" min="0"/><solitaryFeature name="Clear_all"/></binaryRelation></solitaryFeature></binaryRelation><binaryRelation name="R-Notepad"><cardinality max="1" min="1"/><solitaryFeature name="Finder"/></binaryRelation></feature>
</feature-model>
